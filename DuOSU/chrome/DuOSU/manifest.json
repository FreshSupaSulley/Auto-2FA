{
  "name": "DuOSU",
  "description": "Login through Duo Mobile with ease.",
  "version": "1.3.0",
  "manifest_version": 3,
  "permissions": [
    "storage"
  ],
  "content_scripts": [{
    "matches": ["https://*.duosecurity.com/frame/v4/auth/prompt*"],
    "js": ["contentScript.js"]
  }],
  "host_permissions": ["https://*.duosecurity.com/*"],
  "background": {
    "service_worker": "worker.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "/images/duo16.png",
      "32": "/images/duo32.png",
      "48": "/images/duo48.png",
      "128": "/images/duo128.png"
    }
  },
  "icons": {
    "16": "/images/duo16.png",
    "32": "/images/duo32.png",
    "48": "/images/duo48.png",
    "128": "/images/duo128.png"
  }
}
